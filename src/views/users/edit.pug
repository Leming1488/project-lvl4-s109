
extends /layouts/application.pug

block content
  +formFor(f, { url: urlFor('users'), method: 'post' })
    +input(f, 'email')
    +input(f, 'firstName')
    +input(f, 'lastName')
    +button(f)
    input(type ='hidden' name='_method' value='PUT')
    input(type ='hidden' name='id' value= user.id)
